<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.flrjcx.xypt.mapper.EmailRegisterMapper">

    <update id="setStatusOk" parameterType="string">
        update
            users
        set
            status = '1'
        where
            user_id = #{uid}
    </update>

</mapper>
