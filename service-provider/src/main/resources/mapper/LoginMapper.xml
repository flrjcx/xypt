<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.flrjcx.xypt.mapper.LoginMapper">
    <resultMap id="userModel" type="com.flrjcx.xypt.common.model.param.UserVo">
        <id property="user_id" column="userId"/>
        <result property="nick_name" column=""/>
        <result property="nick_pic" column=""/>
        <result property="content" column=""/>
        <result property="sex" column=""/>
        <result property="birthday" column=""/>
        <result property="phone" column=""/>
        <result property="email" column=""/>
        <result property="account" column=""/>
        <result property="password" column=""/>
        <result property="balance" column=""/>
        <result property="fans_num" column=""/>
        <result property="focus_num" column=""/>
        <result property="site" column=""/>
        <result property="school" column=""/>
        <result property="create_time" column=""/>
        <result property="update_time" column=""/>
        <result property="status" column=""/>
    </resultMap>
    <select id="testUserList" resultType="com.flrjcx.xypt.common.model.param.UserVo">
        select
user_id,
nick_name,
nick_pic,
content,
sex,
birthday,
phone,
email,
account,
password,
balance,
fans_num,
focus_num,
site,
school,
create_time,
update_time,
status
from users
    </select>
</mapper>
